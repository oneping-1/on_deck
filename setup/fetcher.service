[Unit]
Description=on_deck_fetcher startup
After=update.target

[Service]
User=on_deck
Type=simple
WorkingDirectory=/home/on_deck/on_deck/on_deck
ExecStart=/home/on_deck/venv/bin/python -u on_deck_fetcher.py
StandardOutput=none
StandardError=none
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
